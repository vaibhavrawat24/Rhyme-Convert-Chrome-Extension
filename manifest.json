{
    "manifest_version": 3,
    "name": "Rhymeify",
    "version": "1.0",
    "description": "Turns headlines into rhymes on Indian newspaper sites.",
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
    ],
 
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://www.hindustantimes.com/"
    ]
  }
  